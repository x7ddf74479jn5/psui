{
  "name": "@x7ddf74479jn5/psui",
  "version": "0.0.5",
  "description": "An UI component library composed with Tailwind CSS and Radix-UI",
  "author": {
    "name": "x7ddf74479jn5",
    "url": "https://github.com/x7ddf74479jn5"
  },
  "repository": {
    "type": "git",
